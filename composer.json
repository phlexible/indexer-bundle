{
    "name": "phlexible/indexer-bundle",
    "description": "phlexible indexer bundle",
    "license": "proprietary",
	"type": "symfony-bundle",
	"keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.3.0",
        "zf1/zend-filter": "~1.12"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5"
    },
    "suggest": {
        "phlexible/indexer-storage-elastica-bundle": "Elastica indexer storage"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.brainbits.net/phlexible-bundles/",
            "options": {
                "ssl": {
                    "allow_self_signed": "true"
                }
            }
        }
    ],
    "minimum-stability": "stable"
}
