{
    "name": "phlexible/indexer-bundle",
    "description": "phlexible indexer bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/collections": "^1.3",
        "phlexible/problem-bundle": "^1.3",
        "symfony/console": "^2.8",
        "symfony/dependency-injection": "^2.8",
        "symfony/event-dispatcher": "^2.8",
        "symfony/http-kernel": "^2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phlexible/queue-bundle": "~1.3"
    },
    "suggest": {
        "phlexible/indexer-storage-elastica-bundle": "Elastica indexer storage"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.2-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
