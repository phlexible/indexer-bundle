{
    "name": "phlexible/indexer-bundle",
    "description": "phlexible indexer bundle",
    "license": "proprietary",
    "type": "symfony-bundle",
    "keywords": ["indexer", "search"],
    "autoload": {
        "psr-4": {
            "Phlexible\\Bundle\\IndexerBundle\\": ""
        }
    },
    "require": {
        "php": ">=5.6.0",
        "doctrine/collections": "~1.0",
        "symfony/event-dispatcher": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "phlexible/queue-bundle": "~1.0"
    },
    "suggest": {
        "phlexible/indexer-storage-elastica-bundle": "Elastica indexer storage"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
